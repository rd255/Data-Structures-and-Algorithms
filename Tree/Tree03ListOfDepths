LinkedList[] ListOfDepths(Tree T) {
	int height = T.height;
	LinkedList[] L = new LinkedList[height];
	Queue<Node> Q = new Queue();
	T.root.height = 0;
	Q.add(T.root);
	while(Q is not empty) {
		Node X = Q.dequeue();
		L[X.height].add(X);
		for each(i in X.adjacent) {
			i.marked=true;
			Q.add(i);
		}
	}
	return L;
} 