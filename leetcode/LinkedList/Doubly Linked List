add(Node head, int data) {
	Node newNode = new Node(data);
	head.prev = newNode;
	newNode.next = head;
	head = newNode;
}

addAfterGivenNode(Node pNode, int data) {
	Node newNode = new Node(data);
	newNode.next = pNode.next;
	newNode.prev = pNode;
	pNode.next.prev = newNode;
	pNode.next = newNode;
}

addAtEnd(Node head,int data) {
	Node newNode = new Node(data);
	while(head.next!=null) head = head.next;
	if(head!=null) head.next = newNode;
	newNode.prev = head;
}
